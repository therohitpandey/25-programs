#include<iostream>
#include<conio.h>

using namespace std;

int main()
{
   
    int a;

  
    cout << "Enter the Number :";
    cin>>a;

    int counter = 1;
    
    do
    {
        cout << "Execute Do While " << counter << " time" << endl;
        counter++;
    } 
    while (counter <= a);

    
    getch();
    return 0;
}
